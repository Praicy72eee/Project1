public abstract class Device {
    private int id;
    private String type;
    private String status;

    public Device(int id, String type) {
        this.id = id;
        this.type = type;
    }

    public int getId() {
        return id;
    }

    public String getType() {
        return type;
    }

    public String getStatus() {
        return status;
    }

    public void turnOn() {
        status = "On";
    }

    public void turnOff() {
        status = "Off";
    }
}
